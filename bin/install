#!/bin/sh

set -e

. $(dirname $0)/../lib/common.sh

PATH="$MYENV_PATH/bin:$PATH"

installdebianfish && omf_install || echo Error while installing fish.

# Setting up bash

[ ! -L ~/.bashrc ] && ln -si $MYENV_PATH/bash/bashrc ~/.bashrc || :
[ ! -L ~/.bash_aliases ] && ln -si $MYENV_PATH/bash/bash_aliases ~/.bash_aliases || :
